{
    "name": "gmorel/state-workflow-bundle",
    "description": "Helping you implementing a complex yet easily maintainable workflow",
    "license": "MIT",
    "keywords": ["state design pattern", "workflow", "finite state machine", "symfony2"],
    "authors": [
        {
            "name": "Guillaume MOREL",
            "email": "guillaume.morel@gmx.com"
        }
    ],
    "type": "symfony-bundle",
    "autoload":     {
        "psr-4": {
            "Gmorel\\StateWorkflowBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "suggest": {
        "spec-gen/state-workflow-spec-gen-bundle": "Specification generator for StateWorkflowBundle"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
